{
  "name": "darkroom-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite --host 127.0.0.1",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "cross-env NODE_ENV=development DEV_SERVER_URL=http://127.0.0.1:5173 concurrently \"npm:dev\" \"wait-on http://127.0.0.1:5173 && npx electron . --dev\"",
    "electron:build": "npm run build && electron-builder",
    "start": "npm run electron:dev"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "konva": "^9.3.22",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.14",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^39.2.4",
    "electron-builder": "^24.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^4.4.9",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.mrw0nder.darkroom",
    "files": [
      "dist/",
      "electron/",
      "node_modules/"
    ],
    "directories": {
      "buildResources": "build"
    }
  }
}