{
  "name": "darkroom-frontend",
  "version": "1.0.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "cross-env NODE_ENV=development concurrently \"npm:dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "start": "npm run electron:dev"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.1",
    "electron": "^29.1.0",
    "vite": "^4.4.9",
    "concurrently": "^8.2.0",
    "wait-on": "^7.0.1",
    "cross-env": "^7.0.3",
    "electron-builder": "^24.0.0"
  },
  "build": {
    "appId": "com.mrw0nder.darkroom",
    "files": [
      "dist/",
      "electron/",
      "node_modules/"
    ],
    "directories": {
      "buildResources": "build"
    }
  }
}