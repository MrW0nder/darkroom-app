{
  "name": "darkroom-frontend",
  "version": "1.0.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite --host 127.0.0.1",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "cross-env NODE_ENV=development DEV_SERVER_URL=http://127.0.0.1:5173 concurrently \"npm:dev\" \"wait-on http://127.0.0.1:5173 && npx electron . --dev\"",
    "electron:build": "npm run build && electron-builder",
    "start": "npm run electron:dev"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^39.2.4",
    "electron-builder": "^24.0.0",
    "vite": "^4.4.9",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.mrw0nder.darkroom",
    "files": [
      "dist/",
      "electron/",
      "node_modules/"
    ],
    "directories": {
      "buildResources": "build"
    }
  }
}